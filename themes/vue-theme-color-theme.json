    {
      "scope": "entity.other.attribute-name.pseudo-element.css, entity.other.attribute-name.pseudo-class.css",
      "settings": {
        "foreground": "#a0b9c4fa"
      }
    },
    {
      "scope": "entity.other.keyframe-offset.percentage.css, meta.property-list.scss",
      "settings": {
        "foreground": "#34eeeed7"
      }
    },
    {
      "scope": "variable.scss, support.function.url.css, entity.other.attribute-name.attribute.scss, punctuation.definition.attribute-selector.begin.bracket.square.scss, punctuation.definition.attribute-selector.end.bracket.square.scss",
      "settings": {
        "foreground": "#88f1ffc2"
      }
