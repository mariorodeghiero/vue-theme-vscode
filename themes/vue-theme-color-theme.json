    /**
     * ----------------------------------
     * Markdown
     * ----------------------------------
     **/
    {
      "name": "markdown",
      "scope": "punctuation.definition.metadata.markdown, punctuation.definition.list.begin.markdown, punctuation.definition.quote.begin.markdown",
      "settings": {
        "foreground": "#ffcc95e7"
      }
    },
    {
      "name": "Horizontal rule",
      "scope": "meta.separator.markdown",
      "settings": {
        "foreground": "#ffcc95e7"
      }
    },
    {
      "name": "Constants",
      "scope": "punctuation.definition.constant.restructuredtext",
      "settings": {
        "foreground": "#ffcc95e7"
      }
    },
    {
      "name": "Blockquotes",
